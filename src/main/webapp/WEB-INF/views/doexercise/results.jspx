<div xmlns:spring="http://www.springframework.org/tags" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:c="http://java.sun.com/jsp/jstl/core" version="2.0">
	<jsp:output omit-xml-declaration="yes"/>
	<spring:message var="title" code="webflow.endstate.title" />
	<div id="_title">
		<h1><c:out value="${i18nService.getText(Exercise, 'title', locale) }"/></h1>
	
		<p><spring:message code="webflow_endstate_message" /></p>
		<spring:message code="webflow_results_points" arguments="${Points.points}" />
	</div>
</div>